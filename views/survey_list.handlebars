<h3>Ongoing Survey List</h3>
<div class="row">
    <div class="col-sm-1"> Index </div>
    <div class="col-sm-2"> Client Name </div>
    <div class="col-sm-4"> Survey Name </div>
    <div class="col-sm-2"> State </div>
    <div class="col-sm-1"> </div>
    <div class="col-sm-1"> </div>
    <div class="col-sm-1"> </div>
</div>
{{#each survey}}
    <div class="row">
        <div class="col-sm-1"> {{@index}} </div>
        <div class="col-sm-2"> {{this.clientName}} </div>
        <div class="col-sm-4"> {{this.surveyName}} </div>
        <div class="col-sm-2"> {{this.state}} </div>
        <div class="col-sm-1"> <a href="/survey/participate?surveyID={{this.id}}&questionnaireID={{this.questionnaireID}}&participantID=CLIENTTOKEN" class="btn btn-default btn-xs">설문 참여</a> </div>
        <div class="col-sm-1"> <a href="/survey/report?surveyID={{this.id}}&questionnaireID={{this.questionnaireID}}&participantID=CLIENTTOKEN&format=html" class="btn btn-default btn-xs">보고서(html)</a> </div>
        <div class="col-sm-1"> <a href="/survey/report?surveyID={{this.id}}&questionnaireID={{this.questionnaireID}}&participantID=CLIENTTOKEN&format=pdf" class="btn btn-default btn-xs">보고서(pdf)</a> </div>
    </div>
{{/each}}